			}
		]
	},
	"condition": {
		"current": 0,
		"list": [
			{
				"name": "首页",
				"path": "pages/index/index",
				"query": "terminal_code=WIN_TEST_123456"
			},
			{
				"name": "我的",
				"path": "pages/user/mine",
				"query": "terminal_code=WIN_TEST_123456"
			}

		]
	},
	"uniIdRouter": {}
